
TYPO3:
  Flow:
    persistence:
      backendOptions:
        host: '<DB_HOST>'
        port: <DB_PORT>
        dbname: '<DB_NAME>'
        user: '<DB_USER>'
        password: '<DB_PASSWORD>'
  Imagine:
    driver: 'Imagick'

  SwiftMailer:
    transport:
      type: 'Swift_MailTransport'

Flowpack:
  ElasticSearch:
    clients:
      default:
        -
          host: '<ELASTICSEARCH_HOST>'
          port: <ELASTICSEARCH_PORT>
